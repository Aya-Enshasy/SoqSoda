<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ProgressCircle" />

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="shadow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="TemplateView">
        <attr name="gnt_template_type" format="reference"/>
    </declare-styleable>
</resources>